<p id="notice"><%= notice %></p>
<%= current_user&.email %>

<% if user_signed_in? %>
  <%= link_to "Log out",
    destroy_user_session_path,
    class: "btn btn-outline-primary",
    method: :delete
  %>
<% else %>
  <%= link_to "Sign in",
    new_user_session_path,
    class: "btn btn-outline-primary"
  %>
<% end %>
